table#topButtons(ng-controller="ContestNavCtrl") 
	tr
		td
			button(ui-sref="contests.list") Back to contests
		td
			button(ng-click="goToDetails()") Details
		td
			button(ng-click="goToJudge()") Judge

hr
h1.title {{contest.title}}
br

table#entryBox
	tr
		td
			label Enter an entry to this contest:
	tr
		td
			textarea(id="entry" ng-model="content")
	tr
		td
			button(ng-click="submit()") Submit Entry
